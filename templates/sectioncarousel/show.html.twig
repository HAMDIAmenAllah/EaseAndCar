{% extends 'admin/baseAdmin.html.twig' %}

{% block title %}Sectioncarousel{% endblock %}

{% block content %}
<div class="flex-column container mt-5">

    <h1>Sectioncarousel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sectioncarousel.id }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ sectioncarousel.image }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ sectioncarousel.titre }}</td>
            </tr>
            <tr>
                <th>Text</th>
                <td>{{ sectioncarousel.text }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('sectioncarousel_index') }}">back to list</a>

    <a href="{{ path('sectioncarousel_edit', {'id': sectioncarousel.id}) }}">edit</a>

    {{ include('sectioncarousel/_delete_form.html.twig') }}
</div>
{% endblock %}
