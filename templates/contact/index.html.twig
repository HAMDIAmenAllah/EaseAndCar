    </div>
  </div>
  <div class="row mt-3">
    <div class="col-md-6 form-group mt-3 mt-md-0">
      {{ form_errors(form.societe) }}
      <div>
        {{
          form_widget(form.societe, {
            attr: {
              placeholder: "Société :"
            }
          })
        }}
      </div>
    </div>

    <div class="col-md-6 form-group mt-3 mt-md-0">
      {{ form_errors(form.subject) }}
      <div>
        {{
          form_widget(form.subject, {
            attr: {
              placeholder: "Subject :"
            }
          })
        }}
      </div>
    </div>
  </div>

  <div class="form-group mt-3">
    {{ form_errors(form.message) }}
    <div>
      {{
        form_widget(form.message, {
          attr: {
            placeholder: "Message :"
          }
        })
      }}
    </div>
  </div>
  
  <center><button class="btn btn-ease mt-2">{{ button_label|default('Envoyer votre message') }}</button></center>

  {# <div class="form-group mt-3">
    {{
      form_widget(form.contact, {
        attr: { placeholder: "envoyer votre message" }
      })
    }}
  </div>
 #}
  <div>
    {{ form_rest(form) }}
  </div>
  {{ form_end(form) }}
</div>


