                              form_widget(form.prenom, {
                                attr: {
                                  placeholder: "Prénom :"
                                }
                              })
                            }}
                          </div>
                        </div>
                      </div>

  <div class="row mt-3">
    <div class="col-md-6 form-group mt-3 mt-md-0">
      {{ form_errors(form.mail) }}
      <div>
        {{
          form_widget(form.mail, {
            attr: {
              placeholder: "Adresse e-mail :"
            }
          })
        }}
      </div>
    </div>

    <div class="col-md-6 form-group mt-3 mt-md-0">
